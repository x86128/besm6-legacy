#: main.c:144
#, c-format
msgid "Options:\n"
msgstr ""

#: main.c:145
#, c-format
msgid "  -x, --native           use native extracode E64\n"
msgstr ""

#: main.c:146
#, c-format
msgid "  -b, --break            break on first cmd\n"
msgstr ""

#: main.c:147
#, c-format
msgid "  -v, --visual           visual mode for debugger\n"
msgstr ""

#: main.c:148
#, c-format
msgid "  -t, --trace            trace all extracodes\n"
msgstr ""

#: main.c:149
#, c-format
msgid "  --trace-e64            trace extracode 064\n"
msgstr ""

#: main.c:150
#, c-format
msgid "  -s, --stats            show statistics for machine instructions\n"
msgstr ""

#: main.c:151
#, c-format
msgid "  --path=dir1:dir2...    specify search path for disk images\n"
msgstr ""

#: main.c:152
#, c-format
msgid ""
"  -p, --output-enable    display printing output (default for batch tasks)\n"
msgstr ""

#: main.c:153
#, c-format
msgid "  -q, --output-disable   no printing output (default for TELE tasks)\n"
msgstr ""

#: main.c:154
#, c-format
msgid "  -o file, --output=file redirect printing output to file\n"
msgstr ""

#: main.c:155
#, c-format
msgid "  --output-raw=file      dump raw output to file\n"
msgstr ""

#: main.c:156
#, c-format
msgid "  --decode-output file   decode raw output file to text\n"
msgstr ""

#: main.c:157
#, c-format
msgid "  -l, --output-latin     use Latin letters for output\n"
msgstr ""

#: main.c:158
#, c-format
msgid "  --output-cyrillic      use Cyrillic letters for output (default)\n"
msgstr ""

#: main.c:159
#, c-format
msgid "  -c file, --punch=file  punch to file\n"
msgstr ""

#: main.c:160
#, c-format
msgid ""
"  --punch-binary         punch in binary format (default dots and holes)\n"
msgstr ""

#: main.c:161
#, c-format
msgid ""
"  --input-encoding=code  set encoding for input files: utf8 koi8 cp1251 "
"cp866\n"
msgstr ""

#: main.c:162
#, c-format
msgid "  --bootstrap            used to generate contents of the system disk\n"
msgstr ""

#: main.c:163
#, c-format
msgid "  --no-insn-check        all words but at addr 0 are treated as insns\n"
msgstr ""


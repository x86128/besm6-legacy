msgstr "Вызов:\n"

#: main.c:141
#, c-format
msgid "  %s [options] <task-file>\n"
msgstr ""

#: main.c:142
#, c-format
msgid "  %s [options] <input-buf-number>\n"
msgstr ""

#: main.c:143
#, c-format
